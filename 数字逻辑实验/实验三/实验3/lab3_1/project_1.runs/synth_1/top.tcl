read_verilog -library xil_defaultlib {
  {D:/number logic/lab3_1/project_1.srcs/sources_1/new/GPU.v}
  {D:/number logic/lab3_1/project_1.srcs/sources_1/new/clkDiv.v}
  {D:/number logic/lab3_1/project_1.srcs/sources_1/new/segMsg.v}
  {D:/number logic/lab3_1/project_1.srcs/sources_1/new/top.v}
}
read_xdc {{D:/number logic/lab3_1/project_1.srcs/constrs_1/new/scroll.xdc}}
